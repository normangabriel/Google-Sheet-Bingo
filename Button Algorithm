function modifyCell(cellname,increase){
  var range = SpreadsheetApp.getActiveSheet().getRange(cellname);
  var toAdd = -1;
  if (increase) {
    toAdd = 1;
  }
  range.setValue(range.getValue() + toAdd);
}

function increaseLine2() {
  modifyCell("H18",true);
}

function increaseLine3() {
  modifyCell("C75",true);
}

function increaseLine4() {
  modifyCell("K75",true);
}

function increaseLine5() {
  modifyCell("S75",true);
}

function increaseLine6() {
  modifyCell("AA75",true);
}

function increaseLine7() {
  modifyCell("AI75",true);
}

function increaseLine8() {
  modifyCell("AQ75",true);
}

function increaseLine9() {
  modifyCell("AY75",true);
}

function increaseLine10() {
  modifyCell("BG75",true);
}

function increaseLine11() {
  modifyCell("BO75",true);
}

function increaseLine12() {
  modifyCell("BW75",true);
}

function increaseLine13() {
  modifyCell("CE75",true);
}

function increaseLine14() {
  modifyCell("CM75",true);
}

function decreaseLine2() {
  modifyCell ("H18",false);
}

function ph() {
  // 1 = row 1, 14 = column 14 = N
  goToSheet("Drink Assignment", 1, 1);
}

function goToSheet(sheetName, row, col) {
  var sheet = SpreadsheetApp.getActive().getSheetByName(sheetName);
  SpreadsheetApp.setActiveSheet(sheet);
  var range = sheet.getRange(row, col)
  SpreadsheetApp.setActiveRange(range);
}

function bb() {
  // 1 = row 1, 14 = column 14 = N
  goToSheet("Bingo", 1, 1);
}

function goToSheet(sheetName, row, col) {
  var sheet = SpreadsheetApp.getActive().getSheetByName(sheetName);
  SpreadsheetApp.setActiveSheet(sheet);
  var range = sheet.getRange(row, col)
  SpreadsheetApp.setActiveRange(range);
}
